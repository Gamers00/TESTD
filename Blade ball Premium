game.StarterGui:SetCore("SendNotification", {
    Title = " Script Made by Gamers!";
    Text = "discord Gamers.wtf";
})

local args = {
    [1] = "Input",
    [2] = "Action",
    [3] = false
}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer("Input","Action",false")
