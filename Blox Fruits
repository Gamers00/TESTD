game.StarterGui:SetCore("SendNotification", {
    Title = "Welcome";
    Text = "Script Made by Gamers!";
    Icon = "rbxassetid://14981795816"
})

wait(1)

local UserInputService = game:GetService("UserInputService")

-- Função para simular cliques automáticos em coordenadas aleatórias
local function clicarAutomatico()
    while true do
        wait(1)  -- Intervalo entre cliques (ajuste conforme necessário)
        
        -- Gera coordenadas aleatórias dentro da área da tela do jogador
        local screenSize = game.Players.LocalPlayer.PlayerGui.AbsoluteSize
        local randomPosition = Vector2.new(math.random(0, screenSize.X), math.random(0, screenSize.Y))

        -- Simula um clique na posição aleatória
        UserInputService.InputBegan:FireClient(game.Players.LocalPlayer, {
            UserInputType = Enum.UserInputType.MouseButton1,
            Position = randomPosition
        })
    end
end

-- Inicia a função clicarAutomatico em uma nova thread
coroutine.wrap(clicarAutomatico)()
